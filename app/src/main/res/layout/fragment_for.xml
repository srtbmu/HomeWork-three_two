<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".FinishFragment"
    android:background="@color/black">




    <TextView
        android:id="@+id/TextDarcnet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="DARCNET"
        android:textColor="@color/white"
        android:textSize="75sp"
        />


    <TextView
        android:id="@+id/TextRegist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/TextDarcnet"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="@color/white"
        android:text="3 этап регистрации"
        android:textStyle="bold"
        android:textSize="25sp"
        android:layout_marginTop="15dp"
        />

    <LinearLayout
        android:id="@+id/LinerName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/TextRegist"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="45dp"
        android:orientation="horizontal"
        >

        <TextView
                android:id="@+id/TextWhatName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:textColor="@color/white"
                android:text="Имя :"
                android:textStyle="bold"
                android:textSize="25sp"
                android:layout_marginTop="15dp"
                />

        <TextView
            android:id="@+id/TextName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:text=" ..."
            android:textColor="@color/white"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/LinerSurname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/LinerName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/TextWhatSurnane"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text="Фамилия :"
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

        <TextView
            android:id="@+id/TextSurname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text=" ..."
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/LinerAge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/LinerSurname"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/TextWhatAge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text="Возраст :"
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

        <TextView
            android:id="@+id/TextAge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text=" ..."
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/LinerFloor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/LinerAge"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/TextWhatFloor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text="Пол :"
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

        <TextView
            android:id="@+id/TextFloor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text=" ..."
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/LinerAdresStudies"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/LinerFloor"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/TextWhatStudies"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text="Адрес учебы :"
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

        <TextView
            android:id="@+id/TextStudies"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text=" ..."
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/LinerAdresWork"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/LinerAdresStudies"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/TextWhatWork"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text="Адрес работы :"
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

        <TextView
            android:id="@+id/TextWork"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/white"
            android:text=" ..."
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            />

    </LinearLayout>

